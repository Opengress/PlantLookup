<data-extraction-rules>
    <cloud-backup>
        <!-- Use <include> and <exclude> to control what is backed up.
        <include .../>
        <exclude .../>
        -->
        <exclude domain="database" path="." />
    </cloud-backup>

    <device-transfer>
<!--        <include .../>-->
<!--        <exclude .../>-->
        <include domain="database" path="." />
    </device-transfer>
</data-extraction-rules>